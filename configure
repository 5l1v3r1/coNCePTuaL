     fi
   fi
   $rm -r conftest*
   LDFLAGS="$save_LDFLAGS"

fi
{ echo "$as_me:$LINENO: result: $lt_cv_prog_compiler_static_works_F77" >&5
echo "${ECHO_T}$lt_cv_prog_compiler_static_works_F77" >&6; }

