###########################################################################

# Attach the coNCePTuaL license to every file we distribute.  Also,
# remove all DISTCLEANFILES from the distribution, as they'll be
# regenerated by configure.  Finally, as a particularly obnoxious
# kludge, rebuild stamp-vti now while we still have write access to
# the doc directory; this prevents "make distcheck" from bombing when
# it tries to regenerate stamp-vti in a read-only doc directory.
dist-hook: $(top_builddir)/LICENSE $(top_srcdir)/makehelper.py
	cd $(distdir) ; $(RM) $(DISTCLEANFILES)
	find $(distdir) -type f | \
	  xargs $(PYTHON) $(top_srcdir)/makehelper.py add-license $(top_srcdir)/LICENSE
	-cd doc ; \
	  $(RM) stamp-vti ; \
	  $(MAKE) $(AM_MAKEFLAGS) stamp-vti
	-$(INSTALL_DATA) doc/stamp-vti $(distdir)/doc

###########################################################################

# Create a LICENSE file from license.html.

$(top_builddir)/LICENSE: $(top_srcdir)/doc/license.html $(top_srcdir)/makehelper.py
	cat $(top_srcdir)/doc/license.html | \
	  $(PYTHON) $(top_srcdir)/makehelper.py license-text > $(top_builddir)/LICENSE

# Output the current value of pkgdatadir for use by
# conceptual.apspec.in in creating a Zero Install Injector wrapper
# script.

show-pkgdatadir:
	@echo $(pkgdatadir)

###########################################################################

# Output configuration parameters that the user may want to override.
# (This target is called by configure.)

showenv:
	@echo '    CC          = $(CC)'
	@echo '    CPPFLAGS    = $(CPPFLAGS)'
	@echo '    CFLAGS      = $(CFLAGS)'
	@echo '    CCLD        = $(CCLD)'
	@echo '    LDFLAGS     = $(LDFLAGS)'
	@echo '    LIBS        = $(LIBS)'
	@echo '    '
	@echo '    MPICC       = $(MPICC)'
	@echo '    MPICPPFLAGS = $(MPICPPFLAGS)'
	@echo '    MPICFLAGS   = $(MPICFLAGS)'
	@echo '    MPILDFLAGS  = $(MPILDFLAGS)'
	@echo '    MPILIBS     = $(MPILIBS)'
	@echo '    '
	@echo '    PYTHON      = $(PYTHON)'

# We can't run the coNCePTuaL interpreter (part of "make check") without
# first creating a few modules it needs.  We then symlink the SWIG
# outputs from the source directory to the target directory because
# that's where "makehelper.py install" expects to find them.  Note that
# we use "test -f" instead of "test -e" because the Bourne shell (at
# least, on a Solaris 9 system I tried) doesn't honor "test -e".
@BUILD_PYMODULE_TRUE@all-local: $(nodist_pkgpython_SCRIPTS) $(SWIG_OUTPUTS)
@BUILD_PYMODULE_TRUE@	for swout in libncptl_wrap.c pyncptl.py ; do \
@BUILD_PYMODULE_TRUE@	  test -f $(top_builddir)/$$swout || $(LN_S) $(top_srcdir)/$$swout $(top_builddir)/$$swout ; \
@BUILD_PYMODULE_TRUE@	done

# Generate a SWIG interface file by processing special comments in
# ncptl.h, ensuring that every function prototype ends in ");" (with
# no intervening line break), converting enumerated types to
# preprocessor defines, and replacing "LOG_AGGREGATE" with "int"
# (which is a lot easier than trying to get SWIG to map the type
# correctly).
@BUILD_PYMODULE_TRUE@libncptl.i.in: ncptl.h.in
@BUILD_PYMODULE_TRUE@	cat $(top_srcdir)/ncptl.h.in | \
@BUILD_PYMODULE_TRUE@	  sed "s,^/[*] SWIG: \\(.*\\) [*]/,\\1," | \
@BUILD_PYMODULE_TRUE@	  sed "s,^extern .*[^;,]\$$,&;," | \
@BUILD_PYMODULE_TRUE@	  $(AWK) -F, '\
@BUILD_PYMODULE_TRUE@	    in_enum && /}/ {in_enum=0; next}; \
@BUILD_PYMODULE_TRUE@	    in_enum {printf "#define %s %d\n", $$1, in_enum++-1}; \
@BUILD_PYMODULE_TRUE@	    /typedef enum {/ {in_enum=1}; \
@BUILD_PYMODULE_TRUE@	    !in_enum' | \
@BUILD_PYMODULE_TRUE@	  sed "s,LOG_AGGREGATE,int,g" | \
@BUILD_PYMODULE_TRUE@	  sed 's,[@]NICS[@],"$(DUMMYNUMBER)",g' | \
@BUILD_PYMODULE_TRUE@	  sed 's,[@]LIBTOOL_CURRENT[@],$(DUMMYNUMBER),g' > $@

# We have SWIG; use it to create libncptl_wrap.c.in and pyncptl.py.
# Older versions of SWIG don't produce pyncptl.py so we have to
# generate it ourself.
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@libncptl_wrap.c.in pyncptl.py __init__.py: libncptl.i.in libncptl_extras.i ncptl_config.py
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@	$(RM) $@
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@	$(SWIG) $(SWIGFLAGS) -python -I$(srcdir) -o libncptl_wrap.tmp libncptl.i.in
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@	$(AWK) '/NCPTL_RUN_TIME_VERSION/ {sub(/$(DUMMYNUMBER)/, "@LIBTOOL_" "CURRENT@")}; \
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@		/NICS/ {sub(/"$(DUMMYNUMBER)"/, "@NICS" "@")}; \
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@		{print}' libncptl_wrap.tmp > libncptl_wrap.c.in
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@	$(RM) libncptl_wrap.tmp
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_TRUE@	env PYTHONPATH=$(top_builddir):$$PYTHONPATH $(PYTHON) $(top_srcdir)/makehelper.py pyncptl

# Issue a warning message if ncptl.h.in is modified but we can't
# rebuild the Python interface to match.
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@libncptl_wrap.c.in pyncptl.py: ncptl.h.in
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@	@echo '========================================================'
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@	@echo 'WARNING: ncptl.h.in was modified but configure could not'
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@	@echo 'locate SWIG.  As a result, the Python interface to the  '
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@	@echo 'coNCePTuaL run-time library may not function correctly. '
@BUILD_PYMODULE_TRUE@@HAVE_SWIG_FALSE@	@echo '========================================================'

# Whether or not we have SWIG we perform all of the necessary
# variable substitutions on libncptl_wrap.c.in to produce
# libncptl_wrap.c.
@BUILD_PYMODULE_TRUE@libncptl_wrap.c: libncptl_wrap.c.in $(top_builddir)/config.status
@BUILD_PYMODULE_TRUE@	$(SHELL) $(top_builddir)/config.status --file=libncptl_wrap.c.ni:libncptl_wrap.c.in
@BUILD_PYMODULE_TRUE@	$(SHELL) $(top_builddir)/config.status --header=libncptl_wrap.c:libncptl_wrap.c.ni

# The preceding commands to generate libncptl_wrap.c leave a stamp
# file lying around.
@BUILD_PYMODULE_TRUE@distclean-local:
@BUILD_PYMODULE_TRUE@	$(RM) stamp-h*

# Use Python's distutils to build the pyncptl package, which interfaces the
# coNCePTuaL run-time library to Python.
@BUILD_PYMODULE_TRUE@install-exec-hook: $(SWIG_OUTPUTS)
@BUILD_PYMODULE_TRUE@	chmod -R u+w $(DESTDIR)$(libdir)
@BUILD_PYMODULE_TRUE@	-env PYTHONPATH=$(top_builddir):$$PYTHONPATH $(PYTHON) $(top_srcdir)/makehelper.py install $(top_builddir) --prefix=$(DESTDIR)$(exec_prefix)

@BUILD_PYMODULE_FALSE@install-exec-hook:
@BUILD_PYMODULE_FALSE@	@:

###########################################################################
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
