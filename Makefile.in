# Create a log file with all headers but no data.
@BUILD_RUN_TIME_LIBRARY_TRUE@empty.log: $(top_srcdir)/writelog
@BUILD_RUN_TIME_LIBRARY_TRUE@	$(top_srcdir)/writelog emptylog-%p.log
@BUILD_RUN_TIME_LIBRARY_TRUE@	mv emptylog-0.log empty.log

